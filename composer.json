{
    "name": "brookeslab/cafevariome",
    "type": "project",
    "description": "Cafe Variome - Health Data Discovery",
    "homepage": "https://cafevariome.org/",
    "license": "MIT",
    "require": {
		"php": "^8.0",
        "ext-sockets": "*",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-bcmath": "*",
        "ext-sodium": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "codeigniter4/framework": "^4.3",
        "components/font-awesome": "^5.15",
        "twbs/bootstrap": "^5.2",
		"elasticsearch/elasticsearch": "^7.17",
        "select2/select2": "^4.0",
        "datatables/datatables": "^1.10",
        "phpoffice/phpspreadsheet": "^1.26",
        "box/spout": "^3.3.0",
        "components/jqueryui": "^1.12",
        "vakata/jstree": "^3.3",
        "tinymce/tinymce": "^5.2.0",
        "laudis/neo4j-php-client": "^2.8",
        "psr18-adapter/guzzle": "^0.1",
        "guzzlehttp/psr7":  "^2.4"
    },
    "require-dev": {
		"fakerphp/faker": "^1.9",
		"mikey179/vfsstream": "^1.6",
		"phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "CodeIgniter\\": "system/",
            "CafeVariomeSetup\\": "Install/",
            "App\\": "app/"
        }
    },
    "scripts": {
        "CVInstall": [
            "@composer dump-autoload",
            "bash Install/install.sh",
            "CafeVariomeSetup\\CVInstaller::InstallDB"
        ]
    },
    "support": {
        "source": "https://github.com/Cafe-Variome/CafeVariome"
    },
    "minimum-stability": "dev"
}