{
    "name": "codeigniter4/codeigniter4",
    "type": "project",
    "description": "The CodeIgniter framework v4",
    "homepage": "https://codeigniter.com",
    "license": "MIT",
    "require": {
		"php": "^7.4||^8.0",
        "codeigniter4/framework": "^4",
        "stevenmaguire/oauth2-keycloak": "^2.2.1",
        "components/font-awesome": "^5.9.0",
        "twbs/bootstrap": "^4.3",
		"elasticsearch/elasticsearch": "7.8",
        "select2/select2": "^4.0.7",
        "datatables/datatables": "^1.10.19",
        "phpoffice/phpspreadsheet": "^1.8.2",
        "box/spout": "2.7.3",
        "components/jqueryui": "^1.12.1",
        "vakata/jstree": "^3.3.8",
        "tinymce/tinymce": "^5.2.0",
        "league/oauth2-client": "*",
        "laudis/neo4j-php-client": "^2.0.8",
        "guzzlehttp/psr7":  "^2.0.0"
        },
    "require-dev": {
		"fakerphp/faker": "^1.9",
		"mikey179/vfsstream": "^1.6",
		"phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "CodeIgniter\\": "system/",
            "CafeVariomeSetup\\": "Install/"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@composer dump-autoload",
            "CodeIgniter\\ComposerScripts::postUpdate"
            
        ],
        "CVInstall": [
            "@composer dump-autoload",
            "bash Install/install.sh",
            "CafeVariomeSetup\\CVInstaller::InstallDB"
        ]
    },
    "support": {
        "forum": "http://forum.codeigniter.com/",
        "source": "https://github.com/codeigniter4/CodeIgniter4",
        "slack": "https://codeigniterchat.slack.com"
    },
    "minimum-stability": "dev"
}